block content
  h1 #{title}
  form(method='POST', action='/list/edit/'+form._id)
    .form-group
    label Name:
      input.form-control(name='title', type='text', value=form.name)
    .form-group
    label Email :
    input.form-control(name='author', type='text', value=form.email)
    .form-group
    input.btn.btn-primary(type='submit', value='Submit')